{
  "manifest_version": 2,
  "name": "Launcher",
  "description": "Quick launch",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}